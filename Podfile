# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'SocialCar' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SocialCar
pod 'Bolts'
pod 'FBSDKCoreKit'
pod 'FBSDKLoginKit'
pod 'FBSDKShareKit'
pod 'Realm'

pod 'Firebase', '~> 4.0.0'
pod 'Firebase/Core', '~> 4.0.0'
pod 'Firebase/Messaging', '~> 4.0.0'
pod 'QMChatViewController'
pod 'Google/SignIn'

  target 'SocialCarTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SocialCarUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
